{
  "name": "ccc-lite",
  "version": "0.1.0",
  "author": "PrettyCoffee",
  "license": "MIT",
  "scripts": {
    "start": "run-s clean serve",
    "build": "run-s clean lint ts-check compile",
    "serve": "parcel src/index.html --open --port 3000 --cache-dir ./.parcel-cache",
    "compile": "parcel build src/index.html --no-source-maps --public-url ./",
    "ts-check": "tsc",
    "clean": "rimraf ./dist && rimraf ./.parcel-cache",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "emotion-rgba": "^0.0.11",
    "preact": "^10.11.2",
    "react-feather": "^2.0.10",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@parcel/packager-raw-url": "^2.8.0",
    "@parcel/transformer-webmanifest": "^2.8.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "babel-plugin-auto-import": "^1.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.27.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gh-pages": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "typescript": "^4.8.4"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  }
}
