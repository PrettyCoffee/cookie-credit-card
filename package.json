{
  "name": "cookie-credit-card",
  "version": "0.1.0",
  "description": "Cookie credit card",
  "author": "PrettyCoffee",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && npm run serve",
    "build": "npm run clean && npm run compile",
    "serve": "parcel src/index.html --open --port 3000",
    "compile": "parcel build src/index.html --no-source-maps",
    "clean": "rimraf ./dist/* && rimraf ./.parcel-cache/*",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "preact": "^10.6.4",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-plugin-auto-import": "^1.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint": "^8.7.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "parcel": "^2.1.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  }
}
